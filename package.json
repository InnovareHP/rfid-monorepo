{
  "name": "dashboard",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:fe": "pnpm --filter fe dev",
    "dev:fe-support": "pnpm --filter fe-support dev",
    "dev:api": "pnpm --filter api dev",
    "start:prod": "pnpm --filter api start:prod",
    "build": "pnpm --filter @dashboard/shared build && pnpm -r --filter=!@dashboard/shared run build",
    "build:shared": "pnpm --filter @dashboard/shared build",
    "build:fe": "pnpm --filter fe build",
    "build:fe-support": "pnpm --filter fe-support build",
    "build:api": "pnpm --filter @dashboard/shared build && pnpm --filter api build",
    "test": "pnpm -r run test",
    "test:fe": "pnpm --filter fe test",
    "lint": "pnpm -r run lint",
    "prisma:generate": "pnpm --filter api prisma:generate",
    "prisma:migrate": "pnpm --filter api prisma:migrate",
    "prisma:push": "pnpm --filter api prisma:push",
    "prisma:studio": "pnpm --filter api prisma:studio"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "bcrypt",
      "esbuild",
      "prisma",
      "protobufjs",
      "@nestjs/core"
    ]
  }
}
